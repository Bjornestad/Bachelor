<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:vm="clr-namespace:Bachelor.ViewModels"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
        x:Class="Bachelor.Views.MainWindowView"
        x:DataType="vm:MainWindowViewModel"
        d:DataContext="{x:Static vm:Design.MainWindowViewModel}"
        Title="MainWindowView">
    <StackPanel>
        <TextBlock Text="{Binding Greeting}" />
        <Button Content="Go to Camera View" Command="{Binding NavigateToBasicCameraCommand}" />
    </StackPanel>
</Window>
